#!/usr/bin/gnuplot -persist
set xrange [-10:10]
set yrange [-10:10]
set zrange [-10:10]
set xlabel 'x-axis'
set ylabel 'y-axis'
set zlabel 'ts: 1/2'
set grid
set hidden3d
set term png
set style line 1 lc rgb "red"
set style line 2 lc rgb "blue"
set view 90, 30, 1, 1
set object 1 rectangle from screen 0,0 to screen 1,1 fillcolor rgb"gray"
set output "printem.101.png"
splot 'data.msh'using 1:2:3 lc 1,\
      'cell.msh'using 1:2:3 notitle lc 1 with lines,\
      'xpC8.msh'using 1:2:3 notitle lc 2

